input {
  lumberjack {
    port => {{ logserver.logstash.lumberjack.port }}
    type => "logs"
    ssl_certificate => "/etc/pki/tls/certs/logstash-forwarder.crt"
    ssl_key => "/etc/pki/tls/private/logstash-forwarder.key"
  }
}
output {
  elasticsearch { host => {{ logserver.elasticsearch.ip }} }
  stdout { codec => json }
}
